<script type="text/javascript" src="../../../../assets/js/jquery.js"></script>
<script>
    //导航条滚动效果
    $(window).scroll(function () {
        if ($(window).scrollTop() >= 102 && $("#menu--subnav-sticky-wrapper .section--subnav").length) {
            $("#menu--subnav-sticky-wrapper").addClass('section--sticky');
            $("#tab__nav").css({
                "position":"fixed",
                "top":"50px",
                "z-index":"99"
            });
        } else {
            $("#menu--subnav-sticky-wrapper").removeClass('section--sticky');
            $("#tab__nav").css({
                "position":"static",
                "top":"auto"
            });
        }
    });
    //展开/关闭 收缩起来的二级菜单
    $(".flexnav__toggler").click(function(){
        $(this).parents(".flexnav").toggleClass("is-open");
    })

    //展开/关闭手机端菜单
    $(".nav-mobile__container .icon-hamburger-menu").click(function(){
        $(".offcanvas").toggleClass("offcanvas--open");
        $(".section--oncanvas").toggleClass("is-disabled");

    })
    //展开/关闭手机端菜单二级菜单
    $('.nav-mobile__tabset li').click(function(){
        var $t = $(this).index();
        if(!$(this).hasClass('.is-active')){
            $(this).addClass('is-active').siblings().removeClass('is-active');
            $('.nav-mobile__tab-holder > ul').eq($t).removeClass('js-hidden').addClass('is-active').siblings().addClass('js-hidden')
        }
    })
    if(!$(".nav-mobile__accordion > li:has('span.icon-arrow-next')").length==0){
        $('.nav-mobile__accordion > li span.icon-arrow-next').click(function(){
            if(!$(this).hasClass('.is-expanded')){
                $(this).parent().addClass('is-active').siblings().removeClass();
                $(this).removeClass('collapsed').addClass('is-expanded');
                $(this).parent().siblings().children('span.icon-arrow-next ').removeClass('is-expanded').addClass('collapsed');
                $(this).next('.nav-mobile__accordion__drop').removeClass('js-hidden').addClass('is-open');
                $(this).parent().siblings().children('.nav-mobile__accordion__drop').removeClass('is-open').addClass('js-hidden');
            }else{
                $(this).parent().removeClass('is-active');
                $(this).removeClass('is-expanded').addClass('collapsed');
                $(this).next('.nav-mobile__accordion__drop').removeClass('is-open').addClass('js-hidden');
            }
        })
    }
    //点击非手机端菜单区域隐藏手机端菜单
    $(document).click(function(e){
        var _conMobile = $('.nav-mobile__container,.side-panel');   // 设置目标区域
        if(!_conMobile.is(e.target) && _conMobile.has(e.target).length === 0){
            $(".offcanvas").removeClass("offcanvas--open");
            $(".section--oncanvas").removeClass("is-disabled");
        }
    });

</script>
