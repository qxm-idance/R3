.text--xlarge {
    @extend %text-size--36;
}
.text--large {
    @extend %text-size--19;
}
.text--med {
    @extend %text-size--16;
}
.text--small {
    @extend %text-size--13;
}
.text--xsmall {
    @extend %text-size--11;
}



