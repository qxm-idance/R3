<script>
    //导航条滚动效果
    $(window).scroll(function(){
        if ($(window).scrollTop()>=102 && $("#menu--subnav-sticky-wrapper .section--subnav").length) {
            $("#menu--subnav-sticky-wrapper").addClass('section--sticky');
        }else{
            $("#menu--subnav-sticky-wrapper").removeClass('section--sticky');
        }
    });
    //product detail flip pic
    $('.flip-container .flip-arrow').click(function(){
        var FlipArrow=$('.flip-container .flip-arrow');
        if(FlipArrow.parent().hasClass('flipped')){
            FlipArrow.parent().removeClass('flipped');
        }else{
            FlipArrow.parent().addClass('flipped');
        }
    });
    //delivery info
    $(".delivery-box--link").click(function(e){
        e.stopPropagation();
        if($('.delivery-box').hasClass('is-selected')){
            $('.delivery-box').removeClass('is-selected');
            $('.product-config--overlay').removeClass('is-active');
        }else{
            $(".product-config__row").removeClass('is-selected');
            $('.delivery-box').addClass('is-selected');
            $('.product-config--overlay').addClass('is-active');
        }
    });
    $(document).click(function(){
        if($(".delivery-box").hasClass("is-selected")){
            $(".delivery-box").removeClass("is-selected");
            $('.product-config--overlay').removeClass('is-active');
        }
    });
    //model-box
    $(document).click(function(){
        if($(".modal-box__overlay").hasClass("modal-box--is-open")){
            $('.modal-box__overlay').removeClass('modal-box--is-open');
        }
    });
    $(".product-calc__minibasket").find('a.color-blue').click(function(e){
        e.stopPropagation();
        $('.modal-box__overlay').addClass('modal-box--is-open');
    });
    $(".modal-box__content .modal-box__close-btn").click(function(){
        $('.modal-box__overlay').removeClass('modal-box--is-open');
    });
    //product config slide
    $(".product-config__row").click(function(e){
        e.stopPropagation();
        if($(this).hasClass('is-selected')){
            $(this).removeClass('is-selected');
            $('.product-config--overlay').removeClass('is-active');

        }else{
            $('.delivery-box').removeClass('is-selected');
            $(this).siblings().removeClass('is-selected');
            $(this).addClass('is-selected');
            $('.product-config--overlay').addClass('is-active');
        }
    });
    $(document).click(function(){
        if($(".product-config__row").hasClass("is-selected")){
            $(".product-config__row").removeClass("is-selected");
            $('.product-config--overlay').removeClass('is-active');
        }
    });

    $(".slide-content").click(function(e){
        e.stopPropagation();
    });
    //product detail tab切换
    $(document).ready(function() {
        //Default Action
        $(".tabpanel__panel").hide(); //Hide all content
        $("ul.tabpanel__list li:first").addClass("is-active").show(); //Activate first tab
        $(".tabpanel__panel:first").show(); //Show first tab content

        //On Click Event
        $("ul.tabpanel__list li").click(function() {
            $("ul.tabpanel__list li").removeClass("is-active"); //Remove any "active" class
            $(this).addClass("is-active"); //Add "active" class to selected tab
            $(".tabpanel__panel").hide(); //Hide all tab content
            var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
            $(activeTab).fadeIn(); //Fade in the active content
            return false;
        });
    });
    //agent view button
    $ (".slide-content__agent").click (function(){
        $ (this).find('span').toggleClass ("icon-unlocked");
    });
    //点击跳转购物车
    $ (".product-buy-btn button").click (function(){
        window.location.href="basket.html";
    });


</script>